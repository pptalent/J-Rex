{
  "name": "RegExpCheck",
  "version": "0.1.0",
  "devDependencies": {
    "angular": "^1.3.15",
    "http-server": "^0.8.0",
    "mustache": "^2.0.0",
    "node-sass": "^3.1.1"
  },
  "scripts": {
    "server": "http-server ./ -p 8888",
    "build:js": "browserify js/main.js > js/bundle.js",
    "build:css": "node-sass --output-style compressed sass/main.scss css/index.min.css",
    "dev": "npm run build:js & npm run build:css & npm run server"
  }
}
